variant: fcos
version: 1.5.0
boot_device:
    luks:
        tang:
            - url: http://10.0.1.1:9090
              thumbprint: OVtBkWq-QtK9t5fhkkdnNAJeYvXLLOGyqqMO4WztbcY
        discard: true
kernel_arguments:
    should_exist:
        - mitigations=auto,nosmt=off
        - selinux=0
    should_not_exist:
        - mitigations=auto,nosmt
passwd:
    users:
        - name: sam
          groups:
            - adm
            - wheel
            - sudo
            - systemd-journal
          ssh_authorized_keys:
            - ssh-ed25519 AAAAC3NzaC1lZDI1NTE5AAAAIFwawprQXEkGl38Q7T0PNseL0vpoyr4TbATMkEaZJTWQ
storage:
    directories:
        - path: /etc/systemd/system/rpm-ostreed.service.d
    files:
        - path: /etc/systemd/system/rpm-ostreed.service.d/http-proxy.conf
          overwrite: true
          contents: |
            [Service]
            Environment="http_proxy=http://squid.samcday.com"
            Environment="https_proxy=http://squid.samcday.com"
        - path: /etc/squid/ca.pem
          overwrite: true
          contents:
            # cryptme
            inline: ENC[AES256_GCM,data:,iv:GHUxg8HoYVilgGcu1geLsx2xFUqhsOOpZoTodaNvUtk=,tag:0ZwVW6nvSZ0xrkjrUqmWUA==,type:str]
        - path: /etc/pki/ca-trust/source/anchors/squid-ca.pem
          overwrite: true
          contents:
            # cryptme
            inline: ENC[AES256_GCM,data:,iv:bpwpPwnhXuZ35tugT4WF2zsYFmjJlJqPryUhMgq6fE8=,tag:rLNcUDtVvKuKWymAlgU6OA==,type:str]
systemd:
    units:
        - name: init-rebase.service
          enabled: true
          contents: |
            [Unit]
            ConditionPathExists=!/etc/zincati/config.d/90-disable.toml
            After=network-online.target coreos-ignition-firstboot-complete.service
            [Service]
            Type=oneshot
            RemainAfterExit=yes
            ExecStart=bash -c 'until ping -W1 -c1 1.1.1.1; do sleep 1s; done; rpm-ostree rebase --reboot ostree-unverified-registry:ghcr.io/samcday/home-cluster-node:latest --bypass-driver'
            [Install]
            WantedBy=multi-user.target
