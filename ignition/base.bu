variant: fcos
version: 1.5.0
boot_device:
    luks:
        tang:
            - url: http://10.0.1.1:9090
              thumbprint: OVtBkWq-QtK9t5fhkkdnNAJeYvXLLOGyqqMO4WztbcY
        discard: true
kernel_arguments:
    should_exist:
        - mitigations=auto,nosmt=off
        - selinux=0
    should_not_exist:
        - mitigations=auto,nosmt
passwd:
    users:
        - name: sam
          groups:
            - adm
            - wheel
            - sudo
            - systemd-journal
          ssh_authorized_keys:
            - ssh-ed25519 AAAAC3NzaC1lZDI1NTE5AAAAIFwawprQXEkGl38Q7T0PNseL0vpoyr4TbATMkEaZJTWQ
systemd:
    units:
        - name: init-rebase.service
          enabled: true
          contents: |
            [Unit]
            ConditionPathExists=!/etc/kubeadm.conf.d/00-base.yaml
            After=network-online.target
            [Service]
            After=coreos-ignition-firstboot-complete.service
            Type=oneshot
            RemainAfterExit=yes
            ExecStart=rpm-ostree rebase --reboot ostree-unverified-registry:ghcr.io/samcday/home-cluster-node:latest --bypass-driver
            [Install]
            WantedBy=multi-user.target
